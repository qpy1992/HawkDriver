<?xml version="1.0" encoding="utf-8"?>
<resources>



    <!--dialog相关-->
    <declare-styleable name="m_Rotate3dAnimation">
        <attr name="m_rollType" format="enum">
            <enum name="x" value="0"/>
            <enum name="y" value="1"/>
            <enum name="z" value="2"/>
        </attr>
        <attr name="m_fromDeg" format="float" />
        <attr name="m_toDeg" format="float" />
        <attr name="m_pivotX" format="fraction"/>
        <attr name="m_pivotY" format="fraction" />
    </declare-styleable>


    <declare-styleable name="m_ProgressWheel">
        <attr name="m_matProg_progressIndeterminate" format="boolean" />
        <attr name="m_matProg_barColor" format="color" />
        <attr name="m_matProg_rimColor" format="color" />
        <attr name="m_matProg_rimWidth" format="dimension" />
        <attr name="m_matProg_spinSpeed" format="float" />
        <attr name="m_matProg_barSpinCycleTime" format="integer" />
        <attr name="m_matProg_circleRadius" format="dimension" />
        <attr name="m_matProg_fillRadius" format="boolean" />
        <attr name="m_matProg_barWidth" format="dimension" />
        <attr name="m_matProg_linearProgress" format="boolean" />
    </declare-styleable>
    

</resources>